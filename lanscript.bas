Attribute VB_Name = "lanscript"
Public langutvonal$, langszint%, utfrmnev$, langhun%
Public langform$(200), langment$(200, 20), langprg$(250), langmodul$(1000), spar$(20), spdb%

Public Sub prglang(frmnev$)
  langszint% = 1
  Call langut
  Call langinit(frmnev$, 1)
  Call langinit(frmnev$, 2)
  Call langinit("modul", 3)
  Call szkriptel(frmnev$)
End Sub

Public Sub frmlang(frmnev$)
  Call langinit(frmnev$, 2)
  Call szkriptel(frmnev$)
End Sub

Public Sub langut()
  lanfi = FreeFile
  Open "C:\auwin\auw-lang.par" For Binary Shared As #lanfi
  langfilm& = LOF(lanfi)
  Close lanfi
  If langfilm& > 0 Then
    lanfi = FreeFile
    Open "C:\auwin\auw-lang.par" For Input As #lanfi
    Input #lanfi, langsorsz%
    Close lanfi
    langhun% = 1
    Select Case langsorsz%
      Case 1: langutvonal = "C:\auwin\lang\hun\":
      Case 2: langutvonal = "C:\auwin\lang\eng\": langhun% = 2
      Case 3: langutvonal = "C:\auwin\lang\ger\": langhun% = 3
      Case 4: langutvonal = "C:\auwin\lang\srb\": langhun% = 4
      Case 5: langutvonal = "C:\auwin\lang\cro\": langhun% = 5
      Case 6: langutvonal = "C:\auwin\lang\chi\": langhun% = 6
      Case Else: langutvonal = "C:\auwin\lang\hun\": langhun% = 1
    End Select
    alfag$ = Dir(langutvonal$)
    If alfag$ = "" Then
      langutvonal$ = "C:\auwin\"
    End If
  Else
    langutvonal$ = "C:\auwin\"
  End If
End Sub

Public Sub langinit(frmnev$, frmod%)
  '--- tömbök feltöltése
  '--- frmnev form vagy programneve vagy modul
  '--- frmod%=1 program (langprg tömb)
  '--- frmod%=2 form (langform$ tömb)
  '--- frmod%=3 modul (langmodul$ tömb)
  utfrmnev$ = frmnev$
  If frmod% = 2 Then
    For iiii4% = 1 To 200
      langment$(iiii4%, langszint%) = langform$(iiii4%)
    Next
    langszint% = langszint% + 1
  End If
  scfil = FreeFile
  If frmod% = 1 Then
    Open langutvonal$ + "auwin.pln" For Input As #scfil
  Else
    Open langutvonal$ + "auwin.lan" For Input As #scfil
  End If
  elkezdte% = 0
  Do
    Line Input #scfil, scrp$
    If Left(scrp$, 1) <> "*" Then
      Call scpar(scrp$, "\")
      If UCase(spar(1)) = UCase(frmnev$) And Val(spar(2)) = frmod% Then
        elkezdte% = 1
        If frmod% = 1 Then
          langprg(Val(spar(3))) = spar(4)
        Else
          If frmod% = 2 Then
            langform(Val(spar(3))) = spar(4)
          Else
            langmodul(Val(spar(3))) = spar(4)
          End If
        End If
      Else
        If elkezdte% = 1 Then Exit Do
      End If
    End If
  Loop While Not EOF(scfil)
  Close scfil
End Sub

Public Sub langclos()
  langszint% = langszint% - 1
  If langszint% < 0 Then langszint% = 0
  If langszint% > 0 Then
    For iiii4% = 1 To 200
      langform$(iiii4%) = langment(iiii4%, langszint%)
    Next
  End If
End Sub

