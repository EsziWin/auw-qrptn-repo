Public Sub scpar(sor$, hatarolo$)
  '--- sor paraméterek szétválasztása
  For p% = 1 To 10: spar$(p%) = " ": Next
  x$ = sor$
  spdb% = 0
  Do
    p% = InStr(x$, hatarolo)
    If p% > 0 Then
      spdb% = spdb% + 1
      spar$(spdb%) = Left$(x$, p% - 1)
      x$ = Mid$(x$, p% + 1)
    Else
      If Len(x$) > 0 Then
        spdb% = spdb% + 1
        spar$(spdb%) = x$
        x$ = ""
      End If
    End If
  Loop While Len(x$) > 0
End Sub

